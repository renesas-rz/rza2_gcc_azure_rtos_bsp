        <pin name="REF50CK0">
          <constraint display="Pin only valid for RMII_MODE" actionOnFail="disable">("${ETHER_MODE}" != 0)</constraint>
        </pin>
        <pin name="RMII0_CRS_DV">
          <constraint display="Pin only valid for RMII_MODE" actionOnFail="disable">("${ETHER_MODE}" != 0)</constraint>
